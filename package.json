{
  "name": "blockchain-developer-bootcamp-final-project",
  "version": "1.0.0",
  "description": "Consensys final project",
  "scripts": {
    "setup-client-contracts": "cp -R ./artifacts/contracts ./client/src && cp -R ./typechain-types ./client/src",
    "deploy:dev": "npx hardhat run --network localhost scripts/deploy.ts && npm run setup-client-contracts",
    "deploy:ropsten": "npx hardhat run --network ropsten scripts/deploy.ts && npm run setup-client-contracts",
    "dev": "npx hardhat run --network localhost scripts/deploy.ts",
    "test": "npx hardhat test",
    "install-all-deps": "npm i && cd ./client && npm i",
    "start-client": "cd client && npm run dev",
    "build-client": "cd client && npm run build",
    "start-local-node": "npx hardhat node"
  },
  "dependencies": {
    "@openzeppelin/contracts": "4.3.3"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "2.0.2",
    "@nomiclabs/hardhat-etherscan": "2.1.7",
    "@nomiclabs/hardhat-waffle": "2.0.1",
    "@typechain/ethers-v5": "8.0.3",
    "@typechain/hardhat": "3.0.0",
    "@types/chai": "4.2.22",
    "@types/mocha": "9.0.0",
    "@types/node": "16.11.7",
    "@types/react-draft-wysiwyg": "^1.13.3",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "chai": "4.3.4",
    "chai-as-promised": "7.1.1",
    "dotenv": "10.0.0",
    "eslint": "8.2.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "5.1.1",
    "ethereum-waffle": "3.4.0",
    "ethers": "5.5.1",
    "hardhat": "2.6.8",
    "hardhat-gas-reporter": "1.0.4",
    "mocha": "9.1.3",
    "prettier": "2.4.1",
    "prettier-plugin-solidity": "1.0.0-beta.19",
    "solhint": "3.3.6",
    "solidity-coverage": "0.7.17",
    "ts-node": "10.4.0",
    "typechain": "6.0.3",
    "typescript": "4.5.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jarretmoses/blockchain-developer-bootcamp-final-project.git"
  },
